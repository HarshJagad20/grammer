{
  "name": "harsh/word-generator",
  "description": "it is a grammer based package which helps to get random verbs, nouns & adjectives",
  "type": "project",
  "authors": [
    {
      "name": "harsh jagad",
      "email": "harshjagad20@gmail.com"
    }
  ],
  "license": "MIT",
  "minimum-stability": "dev",
  "prefer-stable": true,
  "autoload": {
    "psr-4": {
      "Harsh\\WordGenerator\\": "src/"
    }
  },
  "autoload-dev": {
    "psr-4": {
      "Harsh\\WordGenerator\\": "src/",
      "Harsh\\WordGenerator\\Tests\\": "tests"
    }
  },
  "scripts": {
    "test": "./vendor/bin/phpunit",
    "pest": "./vendor/bin/pest",
    "test-f": "vendor/bin/phpunit --filter"
  },
  "extra": {
    "laravel": {
      "providers": [
        "Harsh\\WordGenerator\\WordGeneratorProvider"
      ],
      "aliases": {
        "WordGeneratorFacade": "Harsh\\WordGenerator\\Facades\\WordGeneratorFacade"
      }
    }
  },
  "require-dev": {
    "orchestra/testbench": "^6.0",
    "phpunit/phpunit": "^9.5",
    "pestphp/pest": "^1.21",
    "pestphp/pest-plugin-laravel": "^1.2"
  },
  "config": {
    "allow-plugins": {
      "pestphp/pest-plugin": true,
      "kylekatarnls/update-helper": false
    }
  }
}